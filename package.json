{
  "name": "multi-agent-runtime",
  "version": "2.0.0",
  "description": "Local runtime for multi-agent system with Claude Code CLI integration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "agent": "dist/cli/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
